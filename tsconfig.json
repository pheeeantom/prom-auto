      "module": "ES6",
      "noImplicitAny": true,
      "removeComments": true,
      "preserveConstEnums": true,
      "sourceMap": true,
      "moduleResolution": "node",
      "jsx": "react",
      "allowSyntheticDefaultImports": true,
    },
    "include": ["src/**/*"],
    "exclude": ["**/*.spec.ts"]
  }